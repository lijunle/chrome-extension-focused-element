{
  "manifest_version": 2,
  "name": "Focused Element",
  "description": "Continuously show the current focused element in console.",
  "version": "0.0.1",
  "author": "Junle Li",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "icons": {
    "16": "icons/focus-16.png",
    "32": "icons/focus-32.png",
    "48": "icons/focus-48.png",
    "128": "icons/focus-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/focus-16.png",
      "32": "icons/focus-32.png",
      "48": "icons/focus-48.png",
      "128": "icons/focus-128.png"
    }
  },
  "options_page": "options.html"
}
